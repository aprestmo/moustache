#: settings.php:290
msgid "Settings"
msgstr "Innstillinger"

#. Description of the plugin
#: load.php
msgid "Converts images to more modern formats such as WebP or AVIF during upload."
msgstr "Konverterer bilder til mer moderne bildeformater som WebP og AVIF under opplasting."

#. Plugin Name of the plugin
#: load.php
msgid "Modern Image Formats"
msgstr "Moderne bildeformater"

#: helper.php:220
msgid "The image does not have a valid metadata."
msgstr "Bildet har ikke gyldige metadata."

#: helper.php:187
msgid "The file key is not present on the image data"
msgstr "Filnøkkelen er ikke tilstede i bildedataene"

#: helper.php:142
msgid "The provided mime type is not supported."
msgstr "Angitt MIME-type er ikke støttet."

#: helper.php:138
msgid "The provided mime type is not allowed."
msgstr "Den angitte MIME-typen er ikke tillatt."

#. Author URI of the plugin
#: load.php
msgid "https://make.wordpress.org/performance/"
msgstr "https://make.wordpress.org/performance/"

#. Author of the plugin
#: load.php
msgid "WordPress Performance Team"
msgstr "Ytelsesgruppen i WordPress"

#: helper.php:159
msgid "At least one of the dimensions must be a positive number."
msgstr "Minst én av størrelsene må vøre et positivt tall."

#: helper.php:147
msgid "The original image file does not exists, subsizes are created out of the original image."
msgstr "Den originale bildefilen finnes ikke. Understørrelse er laget fra det originale bildet."

